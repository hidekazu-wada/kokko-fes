  <script>
    // @ts-ignore - Swiper型定義の警告を抑制
    import { Swiper } from "swiper";
    // @ts-ignore - Swiper型定義の警告を抑制
    import { Navigation, Thumbs, FreeMode, EffectFade } from "swiper/modules";
    import "swiper/css";
    import "swiper/css/navigation";
    import "swiper/css/thumbs";
    import "swiper/css/free-mode";
    import "swiper/css/effect-fade";

    document.addEventListener("DOMContentLoaded", () => {
      // サムネイルギャラリーの初期化
      const thumbsGallery = new Swiper("#memory-gallery-thumbs", {
        modules: [FreeMode, Thumbs],
        slidesPerView: "auto",
        spaceBetween: 10,
        freeMode: true,
      });

      // メインスライダーの初期化（フェード効果付き）
      const mainGallery = new Swiper("#memory-modal-main", {
        modules: [Navigation, Thumbs, EffectFade],
        effect: "fade",
        fadeEffect: {
          crossFade: true,
        },
        slidesPerView: "auto",
        navigation: {
          nextEl: "#memory-modal-next",
          prevEl: "#memory-modal-prev",
        },
        thumbs: {
          swiper: thumbsGallery,
        },
      });

      // モーダル機能の実装
      const modal = document.getElementById("memory-modal");
      const galleryItems = document.querySelectorAll(".memory-gallery__item");
      const closeButton = document.querySelector(".memory-modal__close");

      // モーダルを開く関数
      const openModal = (index: number) => {
        if (modal) {
          modal.style.opacity = "1";
          modal.style.visibility = "visible";
          document.body.style.overflow = "hidden";

          // クリックしたサムネイルに対応するスライドに移動
          if (mainGallery && typeof index === "number") {
            mainGallery.slideTo(index, 0);
          }
        }
      };

      // モーダルを閉じる関数
      const closeModal = () => {
        if (modal) {
          modal.style.opacity = "0";
          modal.style.visibility = "hidden";
          document.body.style.overflow = "";
        }
      };

      // ギャラリーアイテムにクリックイベントを追加
      galleryItems.forEach((item, index) => {
        item.addEventListener("click", () => {
          openModal(index);
        });
        // キーボードアクセシビリティ
        item.setAttribute("tabindex", "0");
        item.setAttribute("role", "button");
        item.addEventListener("keydown", (e) => {
          if (
            e instanceof KeyboardEvent &&
            (e.key === "Enter" || e.key === " ")
          ) {
            e.preventDefault();
            openModal(index);
          }
        });
      });

      // 年度フィルタリング機能
      const yearButtons = document.querySelectorAll(
        ".memory-content__year-button",
      );
      let currentYear = parseInt(
        yearButtons[0]?.getAttribute("data-year") || "2025",
      );

      const filterImagesByYear = (year: number) => {
        // ギャラリーアイテムのフィルタリング
        const allGalleryItems = document.querySelectorAll(
          ".memory-gallery__item",
        );
        const allModalItems = document.querySelectorAll(".memory-modal__slide");

        // すべて非表示
        allGalleryItems.forEach((item) => {
          (item as HTMLElement).style.display = "none";
        });
        allModalItems.forEach((item) => {
          (item as HTMLElement).style.display = "none";
        });

        // 選択された年度のみ表示
        const targetGalleryItems = document.querySelectorAll(
          `.memory-gallery__item[data-year="${year}"]`,
        );
        const targetModalItems = document.querySelectorAll(
          `.memory-modal__slide[data-year="${year}"]`,
        );

        targetGalleryItems.forEach((item) => {
          (item as HTMLElement).style.display = "block";
        });
        targetModalItems.forEach((item) => {
          (item as HTMLElement).style.display = "block";
        });

        // Swiper更新
        if (thumbsGallery) {
          thumbsGallery.updateSlides();
          thumbsGallery.updateSize();
          thumbsGallery.updateSlidesClasses();
          thumbsGallery.slideTo(0);
        }
        if (mainGallery) {
          mainGallery.updateSlides();
          mainGallery.updateSize();
          mainGallery.updateSlidesClasses();
          mainGallery.slideTo(0);
        }

        // ギャラリーアイテムのクリックイベントを再設定
        const visibleGalleryItems = document.querySelectorAll(
          `.memory-gallery__item[data-year="${year}"]`,
        );
        visibleGalleryItems.forEach((item, index) => {
          // 既存のイベントリスナーを削除
          const newItem = item.cloneNode(true) as HTMLElement;
          item.parentNode?.replaceChild(newItem, item);

          // 新しいイベントリスナーを追加
          newItem.addEventListener("click", () => {
            openModal(index);
          });
          newItem.setAttribute("tabindex", "0");
          newItem.setAttribute("role", "button");
          newItem.addEventListener("keydown", (e) => {
            if (
              e instanceof KeyboardEvent &&
              (e.key === "Enter" || e.key === " ")
            ) {
              e.preventDefault();
              openModal(index);
            }
          });
        });
      };

      // 年度ボタンのクリックイベント
      yearButtons.forEach((button) => {
        button.addEventListener("click", () => {
          const year = parseInt(button.getAttribute("data-year") || "2025");

          // アクティブクラスの切り替え
          yearButtons.forEach((btn) => {
            btn.classList.remove("memory-content__year-button--active");
            btn.setAttribute("aria-pressed", "false");
          });
          button.classList.add("memory-content__year-button--active");
          button.setAttribute("aria-pressed", "true");

          // フィルタリング実行
          currentYear = year;
          filterImagesByYear(year);
        });
      });

      // 初期表示（最初の年度）
      filterImagesByYear(currentYear);

      // クローズボタンのクリックイベント
      if (closeButton) {
        closeButton.addEventListener("click", closeModal);
      }

      // ESCキーで閉じる
      document.addEventListener("keydown", (e) => {
        if (
          e instanceof KeyboardEvent &&
          e.key === "Escape" &&
          modal &&
          modal.style.visibility === "visible"
        ) {
          closeModal();
        }
      });
    });
  </script>
