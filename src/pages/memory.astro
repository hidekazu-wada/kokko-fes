  <style lang="scss">
    @import "../styles/variables";
    @import "../styles/functions";
    @import "../styles/mixins";

    .memory {
      padding-top: spx(296);
      position: relative;
      min-height: 90vh;
      padding-bottom: spx(99);
      @include tablet-up {
        padding-top: ppx(253 * 1.2);
        padding-bottom: ppx(228 * 1.2);
      }
      @include desktop-up {
        padding-top: ppx(253);
        padding-bottom: ppx(228);
      }

      &__header-wrapper {
        padding-bottom: spx(100);
        @include tablet-up {
          padding-bottom: ppx(130 * 1.2);
        }
        @include desktop-up {
          padding-bottom: ppx(130);
        }
      }
    }

    // Memory Content Section
    .memory-content {
      padding-bottom: spx(100);
      @include tablet-up {
        padding-bottom: ppx(130 * 1.2);
      }
      @include desktop-up {
        padding-bottom: ppx(130);
      }

      &__years {
        display: flex;
        width: spx(680);
        padding: spx(40) spx(30);
        justify-content: center;
        align-items: flex-start;
        align-content: flex-start;
        gap: spx(10);
        flex-wrap: wrap;
        border-radius: spx(30);
        background: var(--base_2, #fcf2c9);
        margin-inline: auto;
        @include tablet-up {
          width: ppx(1350 * 1.2);
          padding: ppx(40 * 1.2) ppx(85 * 1.2);
          gap: ppx(20 * 1.2);
        }
        @include desktop-up {
          width: ppx(1350);
          padding: ppx(40) ppx(85);
          gap: ppx(20);
        }
      }

      &__year-button {
        color: var(--logo_color_3, #c99e2d);
        text-align: center;
        font-family: "Zen Kaku Gothic Antique";
        font-size: spx(24);
        font-style: normal;
        font-weight: 700;
        line-height: 180%;
        letter-spacing: spx(-4.8);
        width: spx(200);
        height: spx(43);
        display: flex;
        align-items: center;
        justify-content: center;
        border: spx(2) solid var(--logo_color_3, #c99e2d);
        background: var(--base_1, #fffcf2);
        border-radius: spx(15);
        transition:
          background 0.3s ease,
          color 0.3s ease;
        white-space: nowrap;
        padding-bottom: 0.1em;
        @include tablet-up {
          font-size: ppx(28 * 1.2);
          letter-spacing: ppx(-8.4 * 1.2);
          width: ppx(220 * 1.2);
          height: ppx(50 * 1.2);
          border: ppx(2 * 1.2) solid var(--logo_color_3, #c99e2d);
          border-radius: ppx(15 * 1.2);
          padding-bottom: 0;
        }
        @include desktop-up {
          font-size: ppx(28);
          letter-spacing: ppx(-8.4);
          width: ppx(220);
          height: ppx(50);
          border: ppx(2) solid var(--logo_color_3, #c99e2d);
          border-radius: ppx(15);
        }

        @include hover {
          &:hover {
            background: var(--base_2, #fcf2c9);
          }
        }

        &--active {
          background: var(--logo_color_3, #c99e2d);
          color: var(--base_1, #fffcf2);

          @include tablet-up {
          }
          @include desktop-up {
          }
          @include hover {
            &:hover {
              background: var(--logo_color_3, #c99e2d);
              color: var(--base_1, #fffcf2);
            }
          }
        }
      }
    }

    // Memory Gallery Section
    .memory-gallery {
      .swiper-wrapper {
        display: grid;
        grid-template-columns: spx(210) spx(210) spx(210);
        gap: spx(10);
        justify-content: center;
        justify-items: center;
        margin-top: spx(40);
        @include tablet-up {
          grid-template-columns:
            ppx(460 * 1.2) ppx(460 * 1.2) ppx(460 * 1.2)
            ppx(460 * 1.2);
          gap: ppx(30 * 1.2);
          margin-top: ppx(130 * 1.2);
        }
        @include desktop-up {
          grid-template-columns: ppx(460) ppx(460) ppx(460) ppx(460);
          gap: ppx(30);
          margin-top: ppx(130);
        }
      }
      &__item {
        width: 100%;
        height: spx(158);
        border-radius: spx(10);
        overflow: hidden;
        cursor: pointer;
        @include tablet-up {
          height: ppx(345 * 1.2);
          border-radius: ppx(20 * 1.2);
        }
        @include desktop-up {
          height: ppx(345);
          border-radius: ppx(20);
        }
        @include hover {
          &:hover {
            img {
              transform: scale(1.1);
            }
          }
        }
      }

      &__picture {
        @include tablet-up {
        }
        @include desktop-up {
        }
      }

      &__image {
        display: block;
        width: 100%;
        height: 100%;
        object-fit: cover;
        transition: transform 0.3s ease;
      }
    }

    // Memory Modal
    .memory-modal {
      position: fixed;
      top: 0;
      left: 0;
      width: 100vw;
      height: 100vh;
      z-index: 1000;
      display: flex;
      align-items: center;
