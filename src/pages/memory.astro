---
// コンポーネント
import PageLayout from "../layouts/PageLayout.astro";
import PageBottomWave from "../components/common/PageBottomWave.astro";
import Breadcrumb from "../components/common/Breadcrumb.astro";
import PageTitle from "../components/common/PageTitle.astro";

// 画像とデータ
import { getImage } from "astro:assets";
import memoryData from "../data/memory.json";

// 動的に画像をインポート
const imageModules = import.meta.glob(
  "../images/memory/**/*.{jpg,jpeg,png,webp}",
);

// 型定義
interface ProcessedImage {
  name: string;
  year: number;
  processed: any;
}

interface YearData {
  year: number;
  images: ProcessedImage[];
}

// すべての年度の画像を処理
const allYearData: YearData[] = [];

for (const yearData of memoryData.years) {
  const yearImages: ProcessedImage[] = [];

  for (const imageName of yearData.images) {
    // いくつかのパターンを試す（.jpg, .png, .webp）
    const extensions = [".jpg", ".png", ".webp"];
    let imageProcessed = false;

    for (const ext of extensions) {
      const baseImageName = imageName.replace(/\.[^/.]+$/, ""); // 拡張子を除去
      const imagePathWithExt = `../images/memory/${yearData.year}/${baseImageName}${ext}`;
      const imageModule = imageModules[imagePathWithExt];

      if (imageModule) {
        const image: any = await imageModule();
        const processedImage = await getImage({
          src: image.default,
          format: "webp",
          widths: [210, 460, 620, 1200],
        });
        yearImages.push({
          name: imageName,
          year: yearData.year,
          processed: processedImage,
        });
        imageProcessed = true;
        break;
      }
    }
  }

  if (yearImages.length > 0) {
    allYearData.push({
      year: yearData.year,
      images: yearImages,
    });
  }
}

// 利用可能な年度のリスト（画像が存在する年のみ）
const availableYears = allYearData.map((d) => d.year).sort((a, b) => b - a);
---

<PageLayout title="思い出｜コッコ祭り 2025｜KOKKO Festival">
  <section class="page-wrapper memory">
    <div class="memory__header-wrapper">
      <PageTitle accent="思" title="い出" subtitle="MEMORY" />
    </div>
    <section class="memory-content">
      <nav class="memory-content__years" aria-label="年度選択">
        {
          availableYears.map((year, index) => (
            <button
              class={`memory-content__year-button ${index === 0 ? "memory-content__year-button--active" : ""}`}
              type="button"
              data-year={year}
              aria-pressed={index === 0 ? "true" : "false"}
            >
              {year
                .toString()
                .split("")
                .map((d) =>
                  d === "0"
                    ? "０"
                    : d === "1"
                      ? "１"
                      : d === "2"
                        ? "２"
                        : d === "3"
                          ? "３"
                          : d === "4"
                            ? "４"
                            : d === "5"
                              ? "５"
                              : d === "6"
                                ? "６"
                                : d === "7"
                                  ? "７"
                                  : d === "8"
                                    ? "８"
                                    : d === "9"
                                      ? "９"
                                      : d,
                )
                .join("")}
            </button>
          ))
        }
      </nav>
      <!-- サムネイルギャラリー（Swiper） -->
      <section
        class="memory-gallery swiper"
        id="memory-gallery-thumbs"
        aria-label="思い出の写真ギャラリー"
      >
        <div class="swiper-wrapper">
          {
            allYearData.map((yearData) =>
              yearData.images.map((image: ProcessedImage, index: number) => (
                <article
                  class="memory-gallery__item swiper-slide"
                  data-year={image.year}
                >
                  <picture class="memory-gallery__picture">
                    <source
                      media="(min-width: 1024px)"
                      srcset={image.processed.srcSet.toString()}
                    />
                    <source
                      media="(min-width: 744px)"
                      srcset={image.processed.srcSet.toString()}
                    />
                    <img
                      src={image.processed.src}
                      alt={`${image.year}年コッコ祭りの様子${index + 1}`}
                      class="memory-gallery__image"
                    />
                  </picture>
                </article>
              )),
            )
          }
        </div>
      </section>
    </section>

    <Breadcrumb currentPageTitle="思い出" />
    <PageBottomWave />
  </section>

  <!-- モーダル -->
  <div
    class="memory-modal"
    id="memory-modal"
    role="dialog"
    aria-modal="true"
    aria-label="写真ギャラリーモーダル"
    style="opacity: 0; visibility: hidden;"
  >
    <div class="memory-modal__overlay">
      <div class="memory-modal__container">
        <!-- 閉じるボタン -->
        <button
          class="memory-modal__close"
          type="button"
          aria-label="モーダルを閉じる"
        >
          <svg
            xmlns="http://www.w3.org/2000/svg"
            width="149"
            height="149"
