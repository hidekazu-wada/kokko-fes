---
// コンポーネント
import PageLayout from "../../layouts/PageLayout.astro";
import PageBottomWave from "../../components/common/PageBottomWave.astro";
import Breadcrumb from "../../components/common/Breadcrumb.astro";

// データ
import categoriesData from "../../data/categories.json";

// 画像
import Hiking from "../../images/common/hiking-illustration.png";
import trees from "../../images/common/three-trees-illustration.png";
import DefaultImage from "../../images/eat/default.png";

// フォーマット
import { getImage } from "astro:assets";

const HikingImage = await getImage({
  src: Hiking,
  format: "webp",
  widths: [206, 300, 367],
});
const treesImage = await getImage({
  src: trees,
  format: "webp",
  widths: [140, 150, 170],
});

// 型定義
interface CategoryItem {
  name: string;
  description: string;
  detailInfo: string;
  image: string;
  images?: string[];
  location: string;
  time: string;
  snsUrl: string;
  price: string;
}

interface CategoryData {
  id: string;
  title: string;
  titleAccent: string;
  subtitle: string;
  description: string;
  items: CategoryItem[];
}

interface CategoriesData {
  [key: string]: CategoryData;
}

export async function getStaticPaths() {
  const typedCategoriesData = categoriesData as CategoriesData;
  const categoryData = typedCategoriesData.eat;

  const paths: any[] = [];

  // eatカテゴリーのアイテムごとに詳細ページのパスを生成
  categoryData.items.forEach((item: CategoryItem) => {
    const detailSlug = item.name.toLowerCase().replace(/\s+/g, "-");

    paths.push({
      params: {
        detail: detailSlug,
      },
      props: {
        categoryData,
        itemData: item,
        categoryId: "eat",
      },
    });
  });

  return paths;
}

const { categoryData, itemData, categoryId } = Astro.props;
const { detail } = Astro.params;

// 動的画像インポート
let formattedImages: any[] = [];
const maxImages = 6;
const imageList = itemData.images || [];

try {
  // 画像モジュールを動的にインポート
  const imagePromises = imageList
    .slice(0, maxImages)
    .map(async (imageName: string) => {
      try {
        const imageModule = await import(`../../images/eat/${imageName}`);
        return await getImage({
          src: imageModule.default,
          format: "webp",
          widths: [87, 170, 620, 900, 1400],
        });
      } catch (error) {
        console.warn(`Image not found: ${imageName}, using default`);
        // デフォルト画像を使用
        return await getImage({
          src: DefaultImage,
          format: "webp",
          widths: [87, 170, 620, 900, 1400],
        });
      }
    });

  formattedImages = await Promise.all(imagePromises);
} catch (error) {
  console.error("Error loading images:", error);
  // エラー時はデフォルト画像を1枚表示
  formattedImages = [
    await getImage({
      src: DefaultImage,
      format: "webp",
      widths: [87, 170, 620, 900, 1400],
    }),
  ];
}

// 画像が0枚の場合はデフォルト画像を1枚表示
if (formattedImages.length === 0) {
  formattedImages = [
    await getImage({
      src: DefaultImage,
      format: "webp",
      widths: [87, 170, 620, 900, 1400],
    }),
  ];
}
